<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical"
    android:id="@+id/dateplus_recycle_layout"
    android:layout_marginTop="10dp"
    android:layout_marginBottom="10dp">

    <!-- 상위직원 1위 _HL -->
    <LinearLayout
        android:id="@+id/dateplus_recycle_innerlayout"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:gravity="center_vertical">

        <TextView
            android:id="@+id/dateplus_recycle_staffNameTv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontFamily="@font/nanumsquareneo_cbd"
            android:text="김유경"
            android:textColor="@color/black"
            android:textSize="13sp"/>
        <View
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_weight="1" />
        <TextView
            android:id="@+id/dateplus_recycle_phoneNumTv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="01012345678"
            android:textColor="@color/black"
            android:textSize="13sp"
            android:layout_gravity="center_horizontal"/>
        <View
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_weight="1" />

    </LinearLayout> <!-- 상위직원 1위 _HL -->

</LinearLayout>
