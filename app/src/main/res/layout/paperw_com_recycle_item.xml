<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical"
    android:id="@+id/paperh_reg_recycle_layout">
    <LinearLayout
        android:id="@+id/paperwlist_reglistL"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:layout_marginTop="10dp"
        android:layout_marginBottom="10dp">
        <!-- 사람이미지 -->
        <ImageView
            android:id="@+id/paperwlist_regpersonIv"
            android:layout_width="50dp"
            android:layout_height="50dp"
            app:srcCompat="@drawable/yicon_stafffemale" />

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:layout_marginLeft="20dp"
            android:gravity="center_vertical"
            android:layout_gravity="center_vertical">
            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal">
                <!-- 이름 [직책] -->
                <TextView
                    android:id="@+id/paperwlist_regnameTv"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="김희범"
                    android:textColor="@color/black"
                    android:textSize="15sp" />
                <!-- 이름 [직책] -->
                <TextView
                    android:id="@+id/paperwlist_regposiTv"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="10dp"
                    android:text="[직원]"
                    android:textColor="#737373"
                    android:textSize="13sp" />
            </LinearLayout>


        </LinearLayout>
        <View
            android:layout_width="wrap_content"
            android:layout_height="0dp"
            android:layout_weight="1" />
        <ImageView
            android:id="@+id/paperwlist_reggoIv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/yicon_go"
            android:layout_gravity="right|center_vertical"/>
    </LinearLayout> <!-- 보건증 등록 직원 리스트 frame -->

</LinearLayout>
